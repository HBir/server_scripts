
if [[ $1 == "--logo" ]]; then
    logo="$(tput setaf 2)
       .~~.   .~~.
      '. \ ' ' / .'$(tput setaf 1)
       .~ .~~~..~.
      : .~.'~'.~. :
     ~ (   ) (   ) ~
    ( : '~'.~.'~' : )
     ~ .~ (   ) ~. ~
      (  : '~' :  )
       '~ .~~~. ~'
           '~'
    $(tput sgr0)"

    echo "$logo"
fi

